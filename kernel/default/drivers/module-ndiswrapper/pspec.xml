    <Source>
        <Name>module-ndiswrapper</Name>
        <Homepage>http://ndiswrapper.sourceforge.net/</Homepage>
        <Packager>
            <Name>PiSi Packages</Name>
            <Email>packages@pisipackages.org</Email>
        </Packager>
        <License>GPLv2</License>
        <IsA>app:console</IsA>
        <IsA>driver</IsA>
        <Summary>A network driver wrapper</Summary>
        <Description>module-ndiswrapper allows you to use Windows XP drivers for WLAN cards without proper Linux drivers.</Description>
        <Archive sha1sum="c461cf8e2c1f9e1f87e2a5c5e717f13d2fef5364" type="targz">mirrors://sourceforge/project/ndiswrapper/stable/1.56/ndiswrapper-1.56.tar.gz</Archive>
        <BuildDependencies>
            <Dependency version="2.6.37.6">kernel-module-headers</Dependency>
        </BuildDependencies>
        <Patches>
            <Patch>makefile.patch</Patch>
            <Patch>ndiswrapper.diff</Patch>
            <Patch>ndiswrapper-2.6.35.patch</Patch>
        </Patches>
    </Source>

    <Package>
        <Name>module-ndiswrapper</Name>
        <RuntimeDependencies>
            <Dependency version="2.6.37.6">kernel</Dependency>
            <Dependency>module-ndiswrapper-userspace</Dependency>
        </RuntimeDependencies>
        <Replaces>
            <Package>ndiswrapper</Package>
        </Replaces>
        <Files>
            <Path fileType="library" permanent="true">/lib/modules</Path>
        </Files>
    </Package>

    <Package>
        <Name>module-ndiswrapper-userspace</Name>
        <Summary>Userspace tools and documentation for ndiswrapper kernel module</Summary>
        <Files>
            <Path fileType="man">/usr/share/man</Path>
            <Path fileType="data">/etc/ndiswrapper</Path>
            <Path fileType="executable">/usr/sbin</Path>
            <Path fileType="executable">/sbin</Path>
            <Path fileType="doc">/usr/share/doc</Path>
        </Files>
    </Package>

