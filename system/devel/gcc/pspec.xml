        <BuildDependencies>
            <Dependency>binutils</Dependency>
            <Dependency>bison</Dependency>
            <Dependency>gettext</Dependency>
            <Dependency>ncurses</Dependency>
            <Dependency>zlib</Dependency>
            <Dependency>gnuconfig</Dependency>
            <Dependency>patch</Dependency>
            <Dependency>sed</Dependency>
            <Dependency>texinfo</Dependency>
            <Dependency>gmp</Dependency>
            <Dependency>mpfr</Dependency>
            <Dependency>libmpc</Dependency>
            <Dependency>ppl</Dependency>
            <Dependency>cloog-ppl</Dependency>
            <Dependency>elfutils</Dependency>
        </BuildDependencies>
        <Patches>
            <Patch>textdomain.patch</Patch>
            <Patch>boehm-gc-getnprocs.patch</Patch>

            <!-- Patches from Redhat -->
            <!-- not needed anymore
            <Patch>redhat/const-dso-handle.patch</Patch>
            <Patch>redhat/hash-style-gnu.patch</Patch>
            -->
            <Patch>redhat/c++-builtin-redecl.patch</Patch>
            <Patch>redhat/gcc45-build-id.patch</Patch>
            <!-- leave the decision to later on
            <Patch>redhat/gcc45-no-add-needed.patch</Patch>
            -->
            <Patch>redhat/gcc45-cloog-dl.patch</Patch>
            <Patch>redhat/gcc45-libgomp-omp_h-multilib.patch</Patch>
            <Patch>redhat/gcc45-libstdc++-docs.patch</Patch>
            <Patch>redhat/gcc45-libtool-no-rpath.patch</Patch>
            <Patch>redhat/gcc45-pr33763.patch</Patch>
            <Patch>redhat/gcc45-pr38757.patch</Patch>
            <!-- merged in, check again
            <Patch>redhat/gcc45-pr44542.patch</Patch>
            -->
            <Patch>redhat/gcc45-rh330771.patch</Patch>

            <!-- Patches from Suse -->
            <Patch>suse/boehm-gc-strict-aliasing.patch</Patch>
            <Patch>suse/gcc-noalias-warn.diff</Patch>
            <Patch>suse/gcc43-no-unwind-tables.diff</Patch>
            <!-- not worth
            <Patch>suse/gcc45-disable-ipa-sra-O2.diff</Patch>
            -->
            <Patch>suse/libjava-no-multilib.diff</Patch>
            <Patch>suse/pr27880.diff</Patch>
            <Patch>suse/pr43270.diff</Patch>
            <Patch>suse/sap303956-uchar.diff</Patch>

            <!-- allow scalar is_va_list_type types -->
            <Patch>pr42375.patch</Patch>

            <!-- If computing a constraint for the rhs handle type punning through unions -->
            <!-- merged in, check again
            <Patch level="2">pr45623.patch</Patch>
            -->

            <!-- Pardus only patches, put error/warning count -->
            <Patch level="1">pr26061.patch</Patch>

            <!-- Add gcc options to output file in PARDUS.OPTs section -->
            <Patch>record_gcc_options.patch</Patch>

            <!-- Fix paths in 64bit -->
            <Patch level="1">pure64.patch</Patch>

            <!-- Fix uClibc dynamic linker paths to suit our package's needs -->
            <Patch level="1">fix-uClibc-loader-paths.patch</Patch>

            <!-- Branch updates -->
            <Patch level="2">svn-updates.patch</Patch>

            <!-- Reduce overhead of dwarf2 location tracking -->
            <Patch level="2">dwarf2_loc_tracking.patch</Patch>
        </Patches>
    </Source>

    <Package>
        <Name>libgcc</Name>
        <IsA>library</IsA>
        <PartOf>system.base</PartOf>
        <Summary>Contains a shared library of support routines used by GCC's runtime, and thus by every program on the system</Summary>
        <Files>
            <Path fileType="config">/etc</Path>
            <Path fileType="library">/usr/lib/libstdc++.so*</Path>
            <Path fileType="library">/usr/lib/libgcc_s.so*</Path>
            <Path fileType="library">/usr/lib32/libstdc++.so*</Path>
            <Path fileType="library">/usr/lib32/libgcc_s.so*</Path>
            <Path fileType="library">/usr/share/gcc-*</Path>
            <Path fileType="library">/usr/share/gdb</Path>
        </Files>
    </Package>

    <Package>
        <Name>libgomp</Name>
        <IsA>library</IsA>
        <Summary>Contains GCC shared support library which is needed for OpenMP 2.5 support</Summary>
        <Files>
            <Path fileType="library">/usr/lib/libgomp.so*</Path>
            <Path fileType="library">/usr/lib/libgomp.spec*</Path>
            <Path fileType="library">/usr/lib32/libgomp.so*</Path>
            <Path fileType="library">/usr/lib32/libgomp.spec*</Path>
            <Path fileType="info">/usr/share/info/libgomp.info</Path>
        </Files>
    </Package>

    <Package>
        <Name>libgfortran</Name>
        <IsA>library</IsA>
        <Summary>Contains GCC shared support library for gfortran</Summary>
        <Files>
            <Path fileType="library">/usr/lib/libgfortran.so*</Path>
            <Path fileType="library">/usr/lib32/libgfortran.so*</Path>
        </Files>
    </Package>

    <Package>
        <Name>libobjc</Name>
        <IsA>library</IsA>
        <Summary>Contains GCC shared support library for objc</Summary>
        <Files>
            <Path fileType="library">/usr/lib/libobjc.so*</Path>
            <Path fileType="library">/usr/lib32/libobjc.so*</Path>
        </Files>
    </Package>

